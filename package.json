{
  "name": "todo-vue",
  "version": "1.0.0",
  "description": "vue + webpack",
  "main": "index.js",
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:client",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js --ext .jsx --ext .vue client/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue client/",
    "precommit": "npm run lint-fix",
    "practice": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.prac.conf.js",
    "dev:server": "cross-env NODE_ENV=development node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanjuan594230/todo-vue.git"
  },
  "author": "renyujuan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juanjuan594230/todo-vue/issues"
  },
  "homepage": "https://github.com/juanjuan594230/todo-vue#readme",
  "devDependencies": {
    "ajv": "^6.0.0",
    "autoprefixer": "^9.1.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
